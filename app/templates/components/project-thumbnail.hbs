  {{#if value.children}}
    <header class="subfolder">
      <h2>{{value.folder}}</h2>
    </header>
    {{#each cvalue in value.children}}
      {{project-thumbnail value=cvalue folder=value.folder project_id=project_id}}      
    {{/each}}  
  {{else}}
    <a {{bind-attr href='bigUrl'}} class="thumb">
      <span class="inner-thumb" {{action 'open'}}>
        <img {{bind-attr src=thumbUrl}}>
      </span>
    </a>
  {{/if}}
